    "### Montagem do circuito\n",
    "q = QuantumRegister(2)\n",
    "c = ClassicalRegister(2)\n",
    "circuito = QuantumCircuit(q, c)\n",
    "circuito.h(q[0])\n",
    "circuito.cx(q[0], q[1])\n",
    "circuito.measure(q[0], c[0])\n",
    "circuito.measure(q[1], c[1])"
   ]
